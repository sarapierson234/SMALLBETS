<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://gtri.gatech.edu"?>
<params>
  <library>OceanParameters_plugin</library>
  <!-- Sound Speed Profile -->
  <technique>Munk</technique>

  <x_length>50</x_length> <!-- Number of Points -->
  <y_length>50</y_length> <!-- Number of Points -->
  <z_length>100</z_length> <!-- Number of Points -->
